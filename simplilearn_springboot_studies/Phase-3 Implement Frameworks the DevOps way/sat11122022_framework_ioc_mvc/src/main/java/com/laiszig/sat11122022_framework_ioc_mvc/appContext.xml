<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="a1" class="com.laiszig.sat11122022_framework_ioc_mvc.model.Address">
        <property name="city" value="Bangalore"></property>
        <property name="state" value="Karnataka"></property>
        <property name="country" value="India"></property>
    </bean>

    <bean id="e1" class="com.laiszig.sat11122022_framework_ioc_mvc.model.Employee" autowire="byType">
        <property name="firstName" value="Dhruvik"></property>
        <property name="lastName" value="Parikh"></property>
<!--        <property name="address" ref="a1"></property>-->
    </bean>
</beans>